{
  "manifest_version": 3,
  "name": "sPrint",
  "version": "3.0",

  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "sPrint",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "<all_urls>",
    "storage"
  ]
}
